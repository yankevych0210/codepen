{
  "singleQuote": true,
  "trailingComma": "es5",
  "parser": "babel",
  "semi": true,
  "printWidth": 120,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "singleQuote": false,
        "trailingComma": "none",
        "semi": false,
        "tabWidth": 2,
        "useTabs": false
      }
    }
  ]
}
